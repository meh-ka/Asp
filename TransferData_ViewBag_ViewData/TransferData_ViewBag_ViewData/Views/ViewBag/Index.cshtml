@using TransferData_ViewBag_ViewData.Models;
@{
    Layout = null;

    // Casting de ViewBag.User en Users
    var user = ViewBag.User as Users;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>ViewBag Page</h1>

    <!-- Affichage des données de ViewBag -->
    <span>Name : </span> @ViewBag.Name
    <br />
    <span>Time : </span> @ViewBag.Time
    <hr />

    <!-- Vérification de la nullité de l'objet "user" -->
    @if (user != null)
    {
        <p>UserId : @user.UserId</p>
        <p>UserName : @user.UserName</p>
        <p>UserFamily : @user.UserFamily</p>
    }
    else
    {
        <p>User information is not available.</p>
    }
</body>
</html>
